{
  "comment": "Test configuration for WebSearch MCP - Copy relevant sections to your MCP client",
  "claude_desktop": {
    "mcpServers": {
      "websearch": {
        "command": "docker",
        "args": ["compose", "exec", "-T", "mcp", "python", "server.py"],
        "cwd": "D:/src/websearch_mcp"
      }
    }
  },
  "docker_network": {
    "mcpServers": {
      "websearch": {
        "command": "docker", 
        "args": [
          "run", "--rm", "-i",
          "--network", "websearch_mcp_default",
          "websearch_mcp-mcp:latest"
        ]
      }
    }
  },
  "python_direct": {
    "mcpServers": {
      "websearch": {
        "command": "python",
        "args": ["services/mcp/server.py"],
        "cwd": "D:/src/websearch_mcp",
        "env": {
          "SEARXNG_URL": "http://localhost:8080",
          "EXTRACTOR_URL": "http://localhost:8055", 
          "REDIS_URL": "redis://localhost:6379"
        }
      }
    }
  }
}