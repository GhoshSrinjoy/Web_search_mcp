{
  "mcpServers": {
    "websearch": {
      "command": "docker",
      "args": [
        "compose", 
        "-f", 
        "docker-compose.yml", 
        "exec", 
        "-T", 
        "mcp", 
        "python", 
        "server.py"
      ],
      "cwd": "D:/src/websearch_mcp",
      "env": {
        "SEARXNG_URL": "http://searxng:8080",
        "EXTRACTOR_URL": "http://extractor:8055", 
        "REDIS_URL": "redis://redis:6379",
        "RATE_LIMIT_PER_DOMAIN": "2",
        "CACHE_TTL": "3600",
        "USE_DISTRIBUTED_RATE_LIMIT": "false"
      }
    }
  }
}